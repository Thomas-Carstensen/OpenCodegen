{
  "name": "opencodegen",
  "version": "0.1.0",
  "description": "Generate typed API clients from OpenAPI specifications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "opencodegen": "./dist/cli/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli/index.js"
  },
  "keywords": [
    "openapi",
    "codegen",
    "typescript",
    "api-client"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "tsx": "^4.21.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "openapi-types": "^12.1.3",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
